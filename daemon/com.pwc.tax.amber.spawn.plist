<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.pwc.tax.amber.spawn</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/libexec/spawn/Spawn</string>
        <string>--config</string>
        <string>/etc/spawn/amberPipe-atom.json</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/spawnout.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/spawnerr.log</string>
    <key>Debug</key>
    <true/>
</dict>
</plist>